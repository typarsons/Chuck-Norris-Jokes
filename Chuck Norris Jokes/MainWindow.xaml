<Window x:Name="window1" x:Class="Chuck_Norris_Jokes.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Chuck_Norris_Jokes"
        mc:Ignorable="d"
        Title="Chuck Norris" Height="450" Width="800">
    <Window.Background>
        <ImageBrush ImageSource="/mountain!.jpg"/>
    </Window.Background>
    <Grid>
        <Label x:Name="lblJoke" Content="Label" HorizontalAlignment="Left" Margin="23,356,0,0" VerticalAlignment="Top" RenderTransformOrigin="6.543,8.432" Height="51" Width="675" Background="#FFBCFFF6"/>
        <ComboBox x:Name="cboCat" HorizontalAlignment="Left" Height="30" Margin="23,143,0,0" VerticalAlignment="Top" Width="207"/>

        <Label x:Name="lblCat" Content="Label" HorizontalAlignment="Left" Height="31" Margin="23,301,0,0" VerticalAlignment="Top" Width="228"/>
        <Button x:Name="btnJoke" Content="Get Joke!" HorizontalAlignment="Left" Height="35" Margin="287,138,0,0" VerticalAlignment="Top" Width="62" Click="Button_Click"/>

    </Grid>
</Window>
